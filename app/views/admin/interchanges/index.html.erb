<p id="notice"><%= notice %></p>

<h1>Listing Interchanges</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interchanges.each do |interchange| %>
      <tr>
        <td><%= interchange.name %></td>
        <td><%= link_to 'Show', [:admin, interchange] %></td>
        <td><%= link_to 'Edit', edit_admin_interchange_path(interchange) %></td>
        <td><%= link_to 'Destroy', [:admin, interchange], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Interchange', new_admin_interchange_path %>
